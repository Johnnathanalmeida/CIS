@model List<string[]>

<div class="row">
    <div class="col-xs-12">
        <h3 class="header smaller lighter blue">
            Resultado da importação completa            
        </h3>
    </div>
</div>

<div class="row">
    <div class="col-xs-12">
        @if (Model != null && Model.Count > 0)
        {
            <table id="dynamic-table" class="table table-striped table-bordered table-hover">
                <thead>
                    <tr>
                        <th>Nome da sheet</th>
                        <th>Itens encontrados</th>
                        <th>Itens criados</th>
                        <th>Status</th>
                    </tr>
                </thead>

                <tbody>

                    @foreach (string[] item in Model)
                    {
                        <tr>
                            <td>@item[0]</td>
                            <td>@item[1]</td>
                            <td>@item[2]</td>
                            <td>Concluído</td>
                        </tr>
                    }
                </tbody>
            </table>
        }
        else
        {
            <div class="alert alert-warning">
                <strong>
                    <i class="ace-icon fa fa-meh-o"></i>
                    Oops!
                </strong>

                Nenhum resultado foi retornado da função de importação completa.
                <br>
            </div>
        }
    </div>
</div>